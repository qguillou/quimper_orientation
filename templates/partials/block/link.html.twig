{% if links|length > 0 %}
    <div class="col-12 col-md-5 col-lg-3 offset-md-1">
        <h5>Liens utiles</h5>

        <ul class="list-unstyled quick-links">
            {% for link in links %}
                {% if is_granted('view', link) %}
                    <li><a target="_BLANK" href="{{ link.link }}"><i class="fa fa-angle-double-right pr-2"></i>{{ link.label }}</a></li>
                {% endif %}
            {% endfor %}
        </ul>
    </div>
{% endif %}
